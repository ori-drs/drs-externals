group "00.params_and_model_pub" {
    cmd "pserver" {
        exec = "bot-param-server $RPG_BASE/code_externals/config/atlas/robot.cfg";
        host = "localhost";
    }

    cmd "model_LR_RR" {
        exec = "rosrun model_client robot_model_publisher $RPG_BASE/code_externals/models/atlas_v5/model_LR_RR.urdf";
        host = "localhost";
    }

}

group "01.state_mit" {

    cmd "joints2frames" {
        exec = "rosrun rpg_control joints2frames";
        host = "localhost";
    }

    cmd "state-sync" {
        exec = "drc-state-sync-atlas -a5";
        host = "localhost";
    }
    cmd "state-sync-simple" {
        exec = "rosrun  motion_estimate state-sync-simple.py";
        host = "localhost";
    }
    cmd "fusion" {
        exec = "rosrun motion_estimate se-fusion -v";
        host = "localhost";
    }
    cmd "yawlock" {
        exec = "se-yawlock -yd";
        host = "localhost";
    }
    cmd "contact-passthrough" {
        exec = "drc-controller-contact-passthrough";
        host = "localhost";
    }
    cmd "director-atlas" {
        exec = "director -c $DRC_BASE/software/config/atlas/robot.cfg -v5";
        host = "localhost";
    }
}


